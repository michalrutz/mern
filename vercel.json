{
        "buildCommand": "cd client && npm install && ./node_modules/vite/bin/vite.js build && cd .. && cd api && npm i",
        "outputDirectory": "client/dist",
        "framework": "vite",
        "rewrites":[
                {
                        "source": "/api/(.*)",
                        "destination": "api.index.js"
                }
        ]
}